{
    "name": "acf/floating-cta",
    "title": "Floating Call To Action ",
    "description": "Floating Call To Action - have a small sign up form or a button stick to the top",
    "category": "Theme Main",
    "icon": "menu",
    "keywords": ["content", "navigation", "menu", "action", "call to action", "engagement", "interaction"],
    "acf": {
        "mode": "preview",
        "renderTemplate": "block.php"
    },
    "supports": {
        "align": [ "left", "right", "wide", "full" ],
        "anchor": false,
        "alignContent": true,
        "jsx": true,
        "color": {
            "text": true,
            "background": true,
            "link": true
        },
        "alignText": true,
        "fullHeight": false,
        "alignWide": true
    },
    "attributes": {
        "backgroundColor": {
            "type": "string",
            "default": "primary"
        },
        "align": {
            "type": "string",
            "default": "wide"
        },
        "alignContent": {
            "type": "string",
            "default": "right"
        },
        "alignText": {
            "type": "string",
            "default": "right"
        },
        "justifyContent": {
            "type": "string",
            "default": "flex-end"
        }
    }
}