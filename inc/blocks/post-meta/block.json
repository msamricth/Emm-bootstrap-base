{
    "name": "acf/post-meta",
    "title": "Post Meta",
    "description": "Meta Details for your post in the form of a block",
    "category": "theme-main",
    "icon": "category",
    "keywords": ["content", "navigation"],
    "acf": {
        "mode": "preview",
        "renderTemplate": "block.php"
    },
    "supports": {
        "align": [ "left", "right", "wide", "full" ],
        "anchor": false,
        "alignContent": true,
        "jsx": true,
        "color": {
            "text": true,
            "background": true,
            "link": true
        },
        "alignText": true,
        "fullHeight": false,
        "alignWide": true
    }
}
